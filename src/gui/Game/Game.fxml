<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.canvas.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane stylesheets="@../../styles/main.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Label fx:id="turnLabel" text="TURN">
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding></Label>
            <ScrollPane fx:id="scrollPane">
               <content>
                  <Canvas fx:id="board" height="397.0" width="200.0" />
               </content></ScrollPane>
            <HBox spacing="50.0">
               <children>
                  <Label fx:id="careerLabel" text="CAREER" />
                  <Label fx:id="salaryLabel" layoutX="10.0" layoutY="10.0" text="SALARY" />
                  <Label fx:id="pathLabel" layoutX="38.0" layoutY="10.0" text="PATH" />
                  <Label fx:id="balanceLabel" layoutX="66.0" layoutY="10.0" text="BALANCE" />
               </children>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
